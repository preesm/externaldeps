<?xml version="1.0" encoding="UTF-8"?>
<site>
	<category-def name="externald_deps" label="External Dependencies">
		<description>
			Third party libraries used by Graphiti/DFTools/Preesm.
		</description>
	</category-def>
	<iu>
		<category name="externald_deps" />
		<query>
			<expression type="match">providedCapabilities.exists(p | p.namespace == 'osgi.bundle')</expression>
		</query>
	</iu>

	<category-def name="source_components" label="Developer Resources">
		<description>
			Developper resources (includes source code).
		</description>
	</category-def>
	<iu>
		<category name="source_components" />
		<query>
			<expression type="match"> id ~= /*sources*/ </expression>
		</query>
	</iu> <!-- -->
</site>
